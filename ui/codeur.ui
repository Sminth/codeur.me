&lt;p style=&quot; margin-top:0px; margin-bottom:28px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px;&quot;&gt;&lt;a href=&quot;http://www.programmr.com/&quot;&gt;&lt;span style=&quot; font-size:16pt; text-decoration: underline; color:#0000ff;&quot;&gt;Programmr&lt;/span&gt;&lt;/a&gt;&lt;/p&gt;
